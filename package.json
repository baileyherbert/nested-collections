		"test": "jest"
	},
	"dependencies": {},
	"devDependencies": {
		"@types/jest": "^27.4.1",
		"@types/node": "^16.11.7",
		"jest": "^27.5.1",
		"rimraf": "^3.0.2",
		"ts-jest": "^27.1.3",
		"ts-node-dev": "^1.1.8",
		"typescript": "^4.6.2"
	},
	"jest": {
		"preset": "ts-jest",
		"testEnvironment": "node",
		"globals": {
			"ts-jest": {
				"tsconfig": "tests/tsconfig.json"
			}
		}
	}
