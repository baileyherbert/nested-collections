{
	"name": "@baileyherbert/nested-collections",
	"version": "dev",
	"description": "Nested maps and sets made easier to work with.",
	"author": "Bailey Herbert <hello+npm@bailey.sh> (https://bailey.sh)",
	"repository": "github:baileyherbert/nested-collections",
	"bugs": "https://github.com/baileyherbert/nested-collections/issues",
	"license": "MIT",
	"main": "dist/main.js",
	"types": "dist/main.d.ts",
	"files": [
